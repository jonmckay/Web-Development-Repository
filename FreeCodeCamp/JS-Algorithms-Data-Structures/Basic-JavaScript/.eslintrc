{
    "rules" : {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-warning-comments": [ 1, {"terms": ["todo"], "location": "anywhere"}],
        "no-console": 1,
        "no-extra-parens": 1,
        "space-before-blocks": [1, "always"]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}